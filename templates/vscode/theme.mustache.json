  "tokenColors": [
    {
      "name": "Language methods",
      "scope": [ "variable.language" ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Comment",
      "scope": [ "comment", "punctuation.definition.comment" ],
      "settings": { "fontStyle": "italic", "foreground": "{{editor-text-minor}}" }
    },
    {
      "name": "Punctuation",
      "scope": [ "punctuation", "punctuation.section.class.end", "meta.tag", "punctuation.definition.tag", "punctuation.separator.inheritance.php", "punctuation.definition.tag.html", "punctuation.definition.tag.begin.html", "punctuation.definition.tag.end.html", "keyword.other.template", "keyword.other.substitution" ],
      "settings": { "foreground": "{{editor-text-minor}}" }
    },
    {
      "name": "Interpolation",
      "scope": [ "punctuation.section.embedded", "variable.interpolation" ],
      "settings": { "foreground": "{{editor-text-default}}" }
    },
    {
      "name": "Keywords",
      "scope": [ "keyword", "keyword.control", "keyword.control.flow", "keyword.control.from", "keyword.control.import", "keyword.control.as", "keyword.other", "keyword.other.using", "keyword.other.namespace", "keyword.other.class", "keyword.other.new", "keyword.other.event", "keyword.other.await", "keyword.other.var", "keyword.other.package", "keyword.other.import", "keyword.other.special-method", "storage.type.ts", "storage.modifier", "storage.type.function" ],
      "settings": { "foreground": "{{editor-text-red}}" }
    },
    {
      "name": "Tag",
      "scope": [ "entity.name.tag", "meta.tag.sgml" ],
      "settings": { "foreground": "{{editor-text-red}}" }
    },
    {
      "name": "Invalid",
      "scope": [ "invalid" ],
      "settings": { "fontStyle": "bold", "foreground": "{{editor-bold-red}}" }
    },
    {
      "name": "Strings",
      "scope": [ "string" ],
      "settings": { "foreground": "{{editor-text-green}}" }
    },

    {
      "name": "Variables, Parameters",
      "scope": [ "variable", "string constant.other.placeholder", "entity.name.variable.parameter", "entity.name.variable.local", "variable.parameter", "meta.block variable.other", "support.other.variable", "variable.language.this", "keyword.other.this"],
      "settings": { "fontStyle": "bold", "foreground": "{{editor-text-default}}" }
    },
    {
      "name": "Inherited Class",
      "scope": [ "entity.other.inherited-class" ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Hash keys",
      "scope": [ "constant.language.symbol.hashkey" ],
      "settings": { "foreground": "{{editor-text-default}}" }
    },
    {
      "name": "Properties",
      "scope": [ "variable.other.object.property" ],
      "settings": { "fontStyle": "normal", "foreground": "{{editor-text-blue}}" }
    },
    {
      "name": "Attributes",
      "scope": [ "entity.other.attribute-name" ],
      "settings": { "fontStyle": "italic", "foreground": "{{editor-text-magenta}}" }
    },
    {
      "name": "Function",
      "scope": [ "entity.name.function", "meta.function-call", "variable.function", "support.function", "meta.class-method.js entity.name.function.js", "variable.function.constructor" ],
      "settings": { "foreground": "{{editor-text-blue}}" }
    },
    {
      "name": "Class, Support",
      "scope": [ "entity.name", "support.type", "support.class", "support.orther.namespace.use.php", "meta.use.php", "support.other.namespace.php", "support.type.sys-types", "storage.type", "storage.modifier.package", "storage.modifier.import" ],
      "settings": { "foreground": "{{editor-text-magenta}}" }
    },
    {
      "name": "Types, Primitives",
      "scope": [ "keyword.type", "storage.type.primitive", "support.type" ],
      "settings": { "foreground": "{{editor-text-magenta}}" }
    },
    {
      "name": "Symbols",
      "scope": ["constant.other.symbol", "constant.other.key", "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"],
      "settings": { "foreground": "{{editor-text-yellow}}" }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [ "constant.numeric", "constant.language", "support.constant", "constant.character", "constant.escape", "keyword.other.unit" ],
      "settings": { "foreground": "{{editor-text-yellow}}" }
    },
    {
      "name": "Regular Expressions",
      "scope": [ "string.regexp" ],
      "settings": { "foreground": "{{editor-text-yellow}}" }
    },
    {
      "name": "Escape Characters",
      "scope": [ "constant.character.escape" ],
      "settings": { "foreground": "{{editor-text-yellow}}" }
    },
    {
      "name": "Inserted",
      "scope": [ "markup.inserted, markup.inserted.git_gutter" ],
      "settings": { "foreground": "{{editor-diff-text-added}}" }
    },
    {
      "name": "Deleted",
      "scope": [ "markup.deleted, markup.deleted.git_gutter" ],
      "settings": { "foreground": "{{editor-diff-text-deleted}}" }
    },
    {
      "name": "Changed",
      "scope": [ "markup.changed, markup.changed.git_gutter" ],
      "settings": { "foreground": "{{editor-diff-text-modified}}" }
    },
    {
      "name": "URL",
      "scope": [ "*url*", "*link*", "*uri*" ],
      "settings": { "fontStyle": "underline" }
    },
    {
      "name": "Italic",
      "scope": [ "markup.italic" ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Bold",
      "scope": [ "markup.bold", "markup.bold string" ],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Bold-Italic",
      "scope": [ "markup.bold markup.italic", "markup.italic markup.bold", "markup.quote markup.bold", "markup.bold markup.italic string", "markup.italic markup.bold string", "markup.quote markup.bold string" ],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Underline",
      "scope": [ "markup.underline" ],
      "settings": { "fontStyle": "underline" }
    },
    {
      "name": "Strike",
      "scope": [ "markup.strike" ],
      "settings": { "fontStyle": "strike" }
    },
    {
      "name": "Quote",
      "scope": [ "markup.quote" ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Heading",
      "scope": [ "markup.heading" ],
      "settings": { "foreground": "{{editor-text-yellow}}" }
    },
    {
      "name": "Markup - Separator (---)",
      "scope": [ "meta.separator" ],
      "settings": { "fontStyle": "bold", "foreground": "{{editor-text-minor}}" }
    },
    {
      "name": "Markdown - Heading Punctuation",
      "scope": [ "heading.1.markdown punctuation.definition.heading.markdown" ],
      "settings": { "foreground": "{{editor-text-blue}}" }
    },
    {
      "name": "Markdown - Heading Punctuation",
      "scope": [ "heading.2.markdown punctuation.definition.heading.markdown" ],
      "settings": { "foreground": "{{editor-text-blue}}d0" }
    },
    {
      "name": "Markdown - Heading Punctuation",
      "scope": [ "heading.3.markdown punctuation.definition.heading.markdown" ],
      "settings": { "foreground": "{{editor-text-blue}}a0" }
    },
    {
      "name": "Markdown - Heading Punctuation",
      "scope": [ "heading.4.markdown punctuation.definition.heading.markdown" ],
      "settings": { "foreground": "{{editor-text-blue}}80" }
    },
    {
      "name": "Markdown - Heading",
      "scope": [ "markup.heading.markdown", "entity.name.section.markdown" ],
      "settings": { "fontStyle": "bold", "foreground": "{{editor-text-yellow}}" }
    },
    {
      "name": "Markdown - Link",
      "scope": [ "string.other.link.title.markdown" ],
      "settings": { "foreground": "{{editor-text-link}}" }
    },
    {
      "name": "Markdown - Link Description (e.g. images)",
      "scope": [ "string.other.link.description.markdown" ],
      "settings": { "foreground": "{{editor-text-green}}" }
    },
    {
      "name": "Markdown - Link URL",
      "scope": [ "markup.underline.link.markdown", "meta.link.inline.markdown markup.underline.link.markdown", "meta.image.inline.markdown markup.underline.link.image.markdown" ],
      "settings": { "foreground": "{{editor-text-minor}}" }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [ "constant.other.reference.link.markdown" ],
      "settings": { "foreground": "{{editor-text-minor}}" }
    },
    {
      "name": "Markdown - Code",
      "scope": [ "markup.inline.raw.string.markdown" ],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markdown - Code",
      "scope": [ "markup.fenced_code.block.markdown", "markup.inline.raw.string.markdown" ],
      "settings": {
        "background": "{{window-background-highlight}}",
        "comment": "background not functional due to https://github.com/Microsoft/vscode/issues/3429"
      }
    },
    {
      "name": "Markdown - List Bullets",
      "scope": [ "punctuation.definition.list.begin.markdown" ],
      "settings": { "foreground": "{{editor-text-red}}" }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [ "source.json meta.structure.dictionary.json support.type.property-name.json" ],
      "settings": { "foreground": "{{editor-text-red}}" }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [ "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json" ],
      "settings": { "foreground": "{{editor-text-red}}" }
